<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>Thomas James</Authors>
		<Company>TPJ</Company>
		<Description>
			Simple encrypt library for both one way encryption using SHA256 and two way encryption using AES
		</Description>
		<PackageTags>encrypt, encryption, SHA256, AES</PackageTags>
		<PackageReleaseNotes>V9.0.0 now runs on .NET 9 see github for more details</PackageReleaseNotes>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Version>9.0.0</Version>
		<PackageId>TPJ.Encrypt</PackageId>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<RepositoryUrl>https://github.com/TPJ11/TPJ.Encrypt</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md" Pack="true" PackagePath="\"/>
		<None Include="..\LICENSE.txt" Pack="true" PackagePath="\"/>
	</ItemGroup>

</Project>
